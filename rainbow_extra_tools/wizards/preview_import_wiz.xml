<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="preview_import_wiz_form_view" model="ir.ui.view">
            <field name="name">preview_import_wiz_form_view</field>
            <field name="model">preview.import.wiz</field>
            <field name="arch" type="xml">
                <form string="Preview products to import">
                    <sheet>
                        <div class="alert alert-info" role="alert">
                            <p><span class="fa fa-info-circle"/>This is calling vault products to import.
                                If nothing when Start, no new ones products to import.</p>
                        </div>
                        <div>
                            <group>
                                <field name="items_to_import" nolabel="1"/>
                            </group>
                        </div>
                    </sheet>
                    <footer>
                        <button name="list_vault_products" type="object" string="Start" class="oe_highlight"/>
                        <button name="list_vault_products" type="object" string="Apply" class="oe_highlight"/>
                        <button special="cancel" string="Cancel"/>
                    </footer>
                </form>
            </field>
        </record>


    </data>
</odoo>